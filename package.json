{
  "name": "chic-modules",
  "type": "module",
  "version": "1.0.5",
  "description": "A familiar styled-like API for working with css-modules in React",
  "author": "Liam Howell <liam@liam.codes>",
  "homepage": "https://github.com/eels/chic-modules#readme",
  "repository": "https://github.com/eels/chic-modules",
  "bugs": "https://github.com/eels/chic-modules/issues",
  "license": "MIT",
  "typings": "./types/index.d.ts",
  "sideEffects": false,
  "main": "./dist/chic-modules.cjs",
  "module": "./dist/chic-modules.module.js",
  "umd:main": "./dist/chic-modules.umd.js",
  "unpkg": "./dist/chic-modules.umd.js",
  "exports": {
    ".": {
      "browser": "./dist/chic-modules.module.js",
      "umd": "./dist/chic-modules.umd.js",
      "import": "./dist/chic-modules.mjs",
      "require": "./dist/chic-modules.cjs"
    }
  },
  "scripts": {
    "benchmark": "yarn upgrade chic-modules --silent && yarn build && node ./benchmarks",
    "build": "microbundle --entry ./src/index.ts --raw --no-sourcemap",
    "changelog": "auto-changelog -b 10 -l 10 -t keepachangelog --hide-credit --sort-commits subject",
    "clean": "rimraf ./dist",
    "format": "prettier './{benchmarks,src,test,types}/**/*.{js,ts,tsx}' --check",
    "format:fix": "yarn format --write",
    "lint": "eslint './{benchmarks,src,test,types}/**/*.{js,ts,tsx}' --cache",
    "lint:fix": "yarn lint --fix",
    "lint:types": "tsc --noEmit",
    "prepack": "yarn clean && yarn build",
    "prepare": "husky install",
    "release": "np --no-release-draft",
    "test": "jest --coverage"
  },
  "browserslist": [
    "defaults"
  ],
  "files": [
    "dist/",
    "types/index.d.ts",
    "types/tags.ts",
    "LICENSE.md",
    "package.json",
    "README.md"
  ],
  "keywords": [
    "css",
    "css-modules",
    "react",
    "styled"
  ],
  "dependencies": {
    "classcat": "^5.0.3",
    "html-attributes": "^1.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "style-vendorizer": "^2.1.1",
    "svg-attributes": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/eslint-parser": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/benchmark": "^2.1.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.4",
    "@types/react": "^17.0.32",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "auto-changelog": "^2.3.0",
    "benchmark": "^2.1.4",
    "chic-modules": "latest",
    "eslint": "^8.1.0",
    "eslint-config-semistandard": "^16.0.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-compat": "^4.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest-dom": "^3.9.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "lint-staged": "^12.0.2",
    "microbundle": "^0.14.1",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0"
  }
}
